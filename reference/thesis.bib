  author = {Wu, Xuejiao and Che, Tao and Li, Xin and Wang, Ninglian and Yang, Xiaofan},
  year = {2018},
  month = nov,
  journal = {Geophysical Research Letters},
  volume = {45},
  number = {22},
  pages = {12331--12339},
  publisher = {Amer Geophysical Union},
