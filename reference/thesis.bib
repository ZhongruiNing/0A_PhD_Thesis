}

@article{pandeyHydrologicalResponseChamelia2019,
  title = {Hydrological Response of {{Chamelia}} Watershed in {{Mahakali Basin}} to Climate Change},
  author = {Pandey, Vishnu Prasad and Dhaubanjar, Sanita and Bharati, Luna and Thapa, Bhesh Raj},
  year = {2019},
  month = feb,
  journal = {Science of the Total Environment},
  volume = {650},
  pages = {365--383},
  issn = {00489697},
  doi = {10.1016/j.scitotenv.2018.09.053},
  urldate = {2023-06-27},
  abstract = {Chamelia (catchment area = 1603 km2 ), a tributary of Mahakali, is a snow-fed watershed in Western Nepal. The watershed has 14 hydropower projects at various stages of development. This study simulated the current and future hydrological system of Chamelia using the Soil and Water Assessment Tool (SWAT). The model was calibrated for 2001--2007; validated for 2008--2013; and then applied to assess streamflow response to projected future climate scenarios. Multi-site calibration ensures that the model is capable of reproducing hydrological heterogeneity within the watershed. Current water balance above the Q120 hydrological station in the forms of precipitation, actual evapotranspiration (AET), and net water yield are 2469 mm, 381 mm and 1946 mm, respectively. Outputs of five Regional Climate Models (RCMs) under two representative concentration pathways (RCPs) for three future periods were considered for assessing climate change impacts. An ensemble of bias-corrected RCM projections showed that maximum temperature under RCP4.5 (RCP8.5) scenario for near-, mid-, and far-futures is projected to increase from the baseline by 0.9 {$^\circ$}C (1.1 {$^\circ$}C), 1.4 {$^\circ$}C (2.1 {$^\circ$}C), and 1.6 {$^\circ$}C (3.4 {$^\circ$}C), respectively. Minimum temperature for the same scenarios and future periods are projected to increase by 0.9 {$^\circ$}C (1.2 {$^\circ$}C), 1.6 {$^\circ$}C (2.5 {$^\circ$}C), and 2.0 {$^\circ$}C (3.9 {$^\circ$}C), respectively. Average annual precipitation under RCP4.5 (RCP8.5) scenario for near-, mid-, and far-futures are projected to increase by 10\% (11\%), 10\% (15\%), and 13\% (15\%), respectively. Based on the five RCMs considered, there is a high consensus for increase in temperature but higher uncertainty with respect to precipitations. Under these projected changes, average annual streamflow was simulated to increase gradually from the near to far future under both RCPs; for instance, by 8.2\% in near-, 12.2\% in mid-, and 15.0\% in far-future under RCP4.5 scenarios. The results are useful for planning water infrastructure projects, in Chamelia and throughout the Mahakali basin, to ensure long-term sustainability under climate change.},
  langid = {english},
  lccn = {1},
  keywords = {/unread},
  file = {D:\zotero\storage\7FEJUXVK\Pandey et al_2019_Hydrological response of Chamelia watershed in Mahakali Basin to climate change.pdf}
}

@article{PanXin19602010NianChangJiangLiuYuJiDuanJiangShuiDeShiKongYanBianJiWeiLaiQuShi2017,
  title = {{1960～2010年长江流域极端降水的时空演变及未来趋势}},
  author = {{潘欣} and {尹义星} and {王小军}},
  year = {2017},
  journal = {长江流域资源与环境},
  volume = {26},
  number = {3},
  pages = {436--444},
  issn = {1004-8227},
  abstract = {本文采用长江流域内分布较均匀、无缺测站点的1960{\textasciitilde}2010年逐日降水资料,借助趋势和突变分析、R/S分析和水文频率分析等方法,研究该流域极端降水的时空演变特征和未来趋势。结果表明:(1)长江流域区域平均气候平均降水量(PAV)、简单日降水强度(PINT)、强降水贡献率(PQ95)、强降水阈值(PF95)、最大1日-10日降水量(PX1D-PX10D)基本均呈上升趋势,中下游各极端降水指数均大于上游,同时,中下游的各指数年际变化比上游更剧烈。(2)PAV与PF95的空间分布类似,但前者在流域中部地区下降、两侧上升,而后者为中部上升、两侧下降;PINT与PQ95的空间分布相似,均为大部分地区呈上升趋势,仅西北部下降。PX1D-PX10D总体上以上升为主,但随着持续时间的增长,下降的区域有明显的扩大,而上升的区域有明显的缩小。(3)未来长江流域极端降水将以现有趋势继续发展,并将以上升趋势为主,流域洪涝灾害风险加大。(4)遂宁站PX1D、安化站PX10D极端降水的频率分析表明,直接采用整体数据计算设计降水量会使结果偏于不安全,对于较长重现期的设计降水表现更显著,因此对于极端降水量发生显著变化的情况需要深入研究,探讨更好的设计降水估计方法。},
  langid = {chinese},
  keywords = {/unread},
  annotation = {39 citations(CNKI)[2023-6-27]{$<$}北大核心, CSSCI, CSCD{$>$}},
  file = {D:\zotero\storage\NZ7HZH97\潘欣 et al_2017_1960～2010年长江流域极端降水的时空演变及未来趋势.pdf}
}

@article{papalexiouGlobalRegionalIncrease2019,
