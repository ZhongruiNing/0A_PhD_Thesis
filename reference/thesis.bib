@article{groganWaterBalanceModel2022,
  title = {Water Balance Model ({{WBM}}) v.1.0.0: A Scalable Gridded Global Hydrologic Model with Water-Tracking Functionality},
  shorttitle = {Water Balance Model ({{WBM}}) v.1.0.0},
  author = {Grogan, Danielle S. and Zuidema, Shan and Prusevich, Alex and Wollheim, Wilfred M. and Glidden, Stanley and Lammers, Richard B.},
  year = {2022},
