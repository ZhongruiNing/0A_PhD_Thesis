@article{wagenerParameterEstimationRegionalization2006,
  title = {Parameter Estimation and Regionalization for Continuous Rainfall-Runoff Models Including Uncertainty},
  author = {Wagener, Thorsten and Wheater, Howard S.},
  year = {2006},
