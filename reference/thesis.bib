}

@article{porkkaNotableShiftsPreindustrial2024,
  title = {Notable Shifts beyond Pre-Industrial Streamflow and Soil Moisture Conditions Transgress the Planetary Boundary for Freshwater Change},
  author = {Porkka, Miina and Virkki, Vili and {Wang-Erlandsson}, Lan and Gerten, Dieter and Gleeson, Tom and Mohan, Chinchu and Fetzer, Ingo and Jaramillo, Fernando and Staal, Arie and {te Wierik}, Sofie and Tobian, Arne and {van der Ent}, Ruud and D{\"o}ll, Petra and Fl{\"o}rke, Martina and Gosling, Simon N. and Hanasaki, Naota and Satoh, Yusuke and M{\"u}ller Schmied, Hannes and Wanders, Niko and Famiglietti, James S. and Rockstr{\"o}m, Johan and Kummu, Matti},
  year = {2024},
