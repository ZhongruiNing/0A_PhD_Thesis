@article{zhuAcceleratedDeclineSnow2022,
  title = {Accelerated Decline of Snow Cover in {{China}} from 1979 to 2018 Observed from Space},
  author = {Zhu, Linglong and Ma, Guangyi and Zhang, Yonghong and Wang, Jiangeng and Tian, Wei and Kan, Xi},
  year = {2022},
