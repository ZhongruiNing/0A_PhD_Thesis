@article{gudmundssonObservationbasedGriddedRunoff2015,
  title = {Towards Observation-Based Gridded Runoff Estimates for {{Europe}}},
  author = {Gudmundsson, L. and Seneviratne, S. I.},
  year = {2015},
  journal = {Hydrology and Earth System Sciences},
