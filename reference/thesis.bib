}

@article{aminjafariDistinctivePatternsWater2024,
  title = {Distinctive {{Patterns}} of {{Water Level Change}} in {{Swedish Lakes Driven}} by {{Climate}} and {{Human Regulation}}},
  author = {Aminjafari, S. and Brown, I. A. and Frappart, F. and Papa, F. and Blarel, F. and Mayamey, F. V. and Jaramillo, F.},
  year = {2024},
  journal = {Water Resources Research},
  volume = {60},
