  title = {Temperature and {{Precipitation Variability}} and {{Its Effects}} on {{Streamflow}} in the {{Upstream Regions}} of the {{Lancang-Mekong}} and {{Nu-Salween Rivers}}},
  author = {Fan, Hui and He, Daming},
  year = {2015},
  month = oct,
  journal = {Journal of Hydrometeorology},
  volume = {16},
  number = {5},
  pages = {2248--2263},
  publisher = {Amer Meteorological Soc},
