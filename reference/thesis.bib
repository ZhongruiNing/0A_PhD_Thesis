@article{herImpactNumbersObservations2015,
  title = {Impact of the Numbers of Observations and Calibration Parameters on Equifinality, Model Performance, and Output and Parameter Uncertainty: {{Parameters}}, {{Observations}}, and {{Uncertainty}}},
  shorttitle = {Impact of the Numbers of Observations and Calibration Parameters on Equifinality, Model Performance, and Output and Parameter Uncertainty},
  author = {Her, Younggu and Chaubey, Indrajeet},
