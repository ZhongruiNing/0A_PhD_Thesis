@article{zhangResponseTibetanPlateau2020,
  title = {Response of {{Tibetan Plateau}} Lakes to Climate Change: {{Trends}}, Patterns, and Mechanisms},
  shorttitle = {Response of {{Tibetan Plateau}} Lakes to Climate Change},
  author = {Zhang, Guoqing and Yao, Tandong and Xie, Hongjie and Yang, Kun and Zhu, Liping and Shum, C. K. and Bolch, Tobias and Yi, Shuang and Allen, Simon and Jiang, Liguang and Chen, Wenfeng and Ke, Changqing},
  year = {2020},
