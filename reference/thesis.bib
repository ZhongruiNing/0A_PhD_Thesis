  author = {{Alvarez-Garreton}, Camila and Mendoza, Pablo A. and Pablo Boisier, Juan and Addor, Nans and Galleguillos, Mauricio and {Zambrano-Bigiarini}, Mauricio and Lara, Antonio and Puelma, Cristobal and Cortes, Gonzalo and Garreaud, Rene and McPhee, James and Ayala, Alvaro},
  year = {2018},
