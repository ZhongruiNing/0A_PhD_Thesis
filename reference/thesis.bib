}

@article{mianabadiGlobalBudykoModel2019,
  title = {A Global {{Budyko}} Model to Partition Evaporation into Interception and Transpiration},
  author = {Mianabadi, Ameneh and {Coenders-Gerrits}, Miriam and Shirazi, Pooya and Ghahraman, Bijan and Alizadeh, Amin},
  year = {2019},
  month = dec,
