  author = {Zhou, Sha and Williams, A. Park and Lintner, Benjamin R. and Berg, Alexis M. and Zhang, Yao and Keenan, Trevor F. and Cook, Benjamin I. and Hagemann, Stefan and Seneviratne, Sonia I. and Gentine, Pierre},
