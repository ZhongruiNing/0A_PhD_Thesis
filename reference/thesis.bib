@article{liVegetationControlWater2013,
  title = {Vegetation Control on Water and Energy Balance within the {{Budyko}} Framework},
  author = {Li, Dan and Pan, Ming and Cong, Zhentao and Zhang, Lu and Wood, Eric},
  year = {2013},
  month = feb,
  journal = {Water Resources Research},
  volume = {49},
  number = {2},
  pages = {969--976},
  publisher = {Amer Geophysical Union},
  address = {Washington},
  issn = {0043-1397, 1944-7973},
  doi = {10.1002/wrcr.20107},
  urldate = {2023-09-07},
  abstract = {Budyko's framework has been widely used to study basin-scale water and energy balances and one of the formulations of the Budyko curve is Fu's equation. The curve shape parameter (sic) in Fu's equation controls how much of the available water will be evaporated given the available energy. Previous studies have found that land surface characteristics significantly affect variations in the parameter (sic). In this study, we focus on the vegetation impact and examine the conditions under which vegetation plays a major role in controlling the variability of (sic). Using data from 26 major global river basins that are larger than 300,000 km(2), the basin-specific (sic) parameter is found to be linearly correlated with the long-term averaged annual vegetation coverage. A simple parameterization for the (sic) parameter based solely on remotely sensed vegetation information is proposed, which improves predictions of annual actual evapotranspiration by reducing the root mean square error (RMSE) from 76 mm to 47 mm as compared to the default (sic) value used in the Budyko curve method. The controlling impact of vegetation on the basin-specific (sic) parameter is diminished in small catchments with areas less than 50,000 km(2), which suggests a scale-dependence of the role of vegetation in affecting water and energy balances. In small catchments, other key ecohydrological processes need to be taken into account in order to fully capture the variability of the (sic) parameter in Fu's equation. Citation: Li, D., M. Pan, Z. Cong, L. Zhang, and E. Wood (2013), Vegetation control on water and energy balance within the Budyko framework, Water Resour. Res., 49, doi: 10.1002/wrcr.20107.},
  langid = {english},
  lccn = {1},
  keywords = {/unread,BUDGET,CLIMATE,MEAN ANNUAL EVAPOTRANSPIRATION,STORAGE},
  annotation = {Web of Science ID: WOS:000317828600021},
