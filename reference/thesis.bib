  lccn = {1},
  keywords = {/unread,Humanities and Social Sciences,multidisciplinary,Science},
  file = {D:\zotero\storage\SU53HBU8\Barnett et al_2005_Potential impacts of a warming climate on water availability in snow-dominated.pdf}
}

