@article{goslingComparisonChangesRiver2017,
  title = {A Comparison of Changes in River Runoff from Multiple Global and Catchment-Scale Hydrological Models under Global Warming Scenarios of 1~{$^\circ$}{{C}}, 2~{$^\circ$}{{C}} and 3~{$^\circ$}{{C}}},
  author = {Gosling, Simon N. and Zaherpour, Jamal and Mount, Nick J. and Hattermann, Fred F. and Dankers, Rutger and Arheimer, Berit and Breuer, Lutz and Ding, Jie and Haddeland, Ingjerd and Kumar, Rohini and Kundu, Dipangkar and Liu, Junguo and {van Griensven}, Ann and Veldkamp, Ted I. E. and Vetter, Tobias and Wang, Xiaoyan and Zhang, Xinxin},
