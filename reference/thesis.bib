@article{qiaoImpactClimateChange2021,
  title = {Impact of {{Climate Change}} on {{Water Availability}} in {{Water Source Areas}} of the {{South-to-North Water Diversion Project}} in {{China}}},
  author = {Qiao, Cuiping and Ning, Zhongrui and Wang, Yan and Sun, Jinqiu and Lin, Qianguo and Wang, Guoqing},
