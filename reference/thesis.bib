@article{wuChangesHeavyPrecipitation2015,
  title = {Changes in Heavy Precipitation and Floods in the Upstream of the {{Beijiang River}} Basin, {{South China}}: {{CHANGES IN HEAVY PRECIPITATION AND FLOODS IN THE BEIJIANG RIVER BASIN}}},
  shorttitle = {Changes in Heavy Precipitation and Floods in the Upstream of the {{Beijiang River}} Basin, {{South China}}},
  author = {Wu, Chuanhao and Huang, Guoru},
