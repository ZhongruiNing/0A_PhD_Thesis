@article{qinAgriculturalRisksChanging2020,
  title = {Agricultural Risks from Changing Snowmelt},
  author = {Qin, Yue and Abatzoglou, John T. and Siebert, Stefan and Huning, Laurie S. and AghaKouchak, Amir and Mankin, Justin S. and Hong, Chaopeng and Tong, Dan and Davis, Steven J. and Mueller, Nathaniel D.},
  year = {2020},
