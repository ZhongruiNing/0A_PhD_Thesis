  author = {Guan, Xiaoxiang and Zhang, Jianyun and Yang, Qinli and Tang, Xiongpeng and Liu, Cuishan and Jin, Junliang and Liu, Yue and Bao, Zhenxin and Wang, Guoqing},
  year = {2020},
  month = dec,
  journal = {Remote Sensing},
  volume = {12},
  number = {24},
  pages = {4023},
