@article{wangVanishingGlaciersSoutheast2021,
  title = {Vanishing {{Glaciers}} at {{Southeast Tibetan Plateau Have Not Offset}} the {{Declining Runoff}} at {{Yarlung Zangbo}}},
  author = {Wang, Yuanwei and Wang, Lei and Zhou, Jing and Yao, Tandong and Yang, Wei and Zhong, Xiaoyang and Liu, Ruishun and Hu, Zhidan and Luo, Lun and Ye, Qinghua and Chen, Ningsheng and Ding, Haitao},
  year = {2021},
  journal = {Geophysical Research Letters},
  volume = {48},
  number = {21},
  pages = {e2021GL094651},
  issn = {1944-8007},
  doi = {10.1029/2021GL094651},
  urldate = {2024-03-14},
  abstract = {The Third Pole experiences accelerated glacier retreating particularly in the eastern-Himalaya, coinciding with a decrease of monsoon-precipitation in the early 21st century. The extent to which the vanishing abundant maritime glaciers buffer the declining precipitation-runoff remains unclear. Here, with a state-of-the-art enthalpy-based distributed cryosphere-hydrology model and first-hand hydrometeorology observations at Motuo (latest accessible Chinese county), we carefully examine the Yarlung Zangbo basin along Himalayas. We find that during 1998--2019, the rising downstream runoff (lower Nuxia; +6.40 {\texttimes} 108 m3/yr) offsets the dropping upstream runoff (upper Nuxia; -6.89 {\texttimes} 108 m3/yr); however, only the marginal contribution from the vanishing eastern-Himalaya and Nyainq{\^e}ntanglha glaciers. During 1998--2019, dry upstream illustrates limited glacier melt (15.7 mm/yr) with dominated snow melt (78.8 mm/yr); while much larger at humid downstream (144.8 mm/yr for glacier melt and 219.1 mm/yr for snow melt). From 1981 to 2019, we observe glacier-to-snow melt transition in both upstream and downstream due to glacier degradation and growing nonmonsoon-season precipitation.},
  copyright = {{\copyright} 2021. American Geophysical Union. All Rights Reserved.},
  langid = {english},
  file = {D:\zotero\storage\CINFBWTL\Wang et al_2021_Vanishing Glaciers at Southeast Tibetan Plateau Have Not Offset the Declining.pdf}
}

