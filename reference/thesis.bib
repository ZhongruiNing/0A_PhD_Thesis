@article{ayzelEffectCalibrationData2021,
  title = {The Effect of Calibration Data Length on the Performance of a Conceptual Hydrological Model versus {{LSTM}} and {{GRU}}: {{A}} Case Study for Six Basins from the {{CAMELS}} Dataset},
