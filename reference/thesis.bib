@article{kimApplicationParameterEstimation2008,
  title = {Application of Parameter Estimation and Regionalization Methodologies to Ungauged Basins of the {{Upper Blue Nile River Basin}}, {{Ethiopia}}},
  author = {Kim, Ungtae and Kaluarachchi, Jagath J.},
  year = {2008},
