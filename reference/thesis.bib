@article{guLargeAnomaliesFuture2023,
  title = {Large Anomalies in Future Extreme Precipitation Sensitivity Driven by Atmospheric Dynamics},
  author = {Gu, Lei and Yin, Jiabo and Gentine, Pierre and Wang, Hui-Min and Slater, Louise J. and Sullivan, Sylvia C. and Chen, Jie and Zscheischler, Jakob and Guo, Shenglian},
