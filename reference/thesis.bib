@article{dengModellingTimevariantParameters2019,
  title = {Modelling Time-Variant Parameters of a Two-Parameter Monthly Water Balance Model},
  author = {Deng, Chao and Liu, Pan and Wang, Weiguang and Shao, Quanxi and Wang, Dingbao},
  year = {2019},
  month = jun,
  journal = {Journal of Hydrology},
  volume = {573},
  pages = {918--936},
  publisher = {Elsevier},
  address = {Amsterdam},
  issn = {0022-1694, 1879-2707},
  doi = {10.1016/j.jhydrol.2019.04.027},
  urldate = {2024-03-24},
  abstract = {Parameters are usually assumed to be stationary, and regarded as constants in hydrological modelling. However, studies have shown that model parameters may vary temporally due to human activities, and climate variability and changes. This study proposes a framework for quantifying hydrological model parameters as functions of time-variant catchment properties, aiming to improve the capability of capturing hydrograph and the extra-polative ability of hydrological models under a changing environment. The framework includes four steps: (1) estimating model parameters by using an ensemble Kalman filter based on the observations; (2) analyzing correlations between estimated parameter values and catchment properties; (3) constructing a set of parameter functions based on the identified catchment properties and the proposed functional forms; and (4) selecting and testing the best-performed model. As a demonstration of the framework, the monthly water balance for Tongtianhe and Ganjiang basins in China with different climate are simulated using a two-parameter monthly water balance model. Results from the Tongtianhe Basin show that considering the time-variant evapotranspiration parameter (C) brings improvement in low flows when water storage capacity (SC) is treated as constant since there is no significant improvement in runoff simulation when SC is treated as a time-variant parameter. The application to Ganjiang Basin shows that reasonable improvements are achieved when both C and SC are treated as time-variant, especially in low and high flows. The presented framework provides a useful tool for estimating time-variant parameters of hydrological models, thereby leading to more reliable model predictions under a changing environment.},
  langid = {english},
  keywords = {Catchment properties,CATCHMENTS,CLIMATE-CHANGE,DATA ASSIMILATION,Ensemble Kalman filter,ENSEMBLE KALMAN FILTER,HYDROLOGICAL IMPACTS,Hydrological modelling,Monthly water balance model,PERFORMANCE,RAINFALL-RUNOFF MODEL,RIVER-BASIN,STATE,STREAMFLOW,Time-variant parameter},
  annotation = {Web of Science ID: WOS:000474327800072},
  file = {D:\zotero\storage\K4MED63T\Deng et al_2019_Modelling time-variant parameters of a two-parameter monthly water balance model.pdf}
}

@article{dengTemporalVariationScaling2018,
