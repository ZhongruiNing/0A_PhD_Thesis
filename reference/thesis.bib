}

@article{vishwakarmaDownscalingGRACETotal2021,
  title = {Downscaling {{GRACE}} Total Water Storage Change Using Partial Least Squares Regression},
  author = {Vishwakarma, Bramha Dutt and Zhang, Jinwei and Sneeuw, Nico},
