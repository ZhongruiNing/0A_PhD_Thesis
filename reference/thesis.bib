  lccn = {1},
  keywords = {/unread,ADAPTATION,BIAS CORRECTION,climate change,CLIMATE-CHANGE,CMIP5,CMIP6,comparison,DATASET,ENSEMBLE,EXTREMES,hydrological impacts,North America,PRECIPITATION,REGIONAL CLIMATE,TEMPERATURE},
  annotation = {Web of Science ID: WOS:000839081200001},
