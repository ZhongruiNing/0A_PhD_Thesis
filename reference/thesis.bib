@article{liuGlobalAttributionRunoff2019,
  title = {Global {{Attribution}} of {{Runoff Variance Across Multiple Timescales}}},
  author = {Liu, Jianyu and Zhang, Qiang and Feng, Shuyun and Gu, Xihui and Singh, Vijay P. and Sun, Peng},
