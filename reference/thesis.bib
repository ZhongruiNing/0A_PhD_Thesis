@article{zhuConstrainedTropicalLand2023,
  title = {Constrained Tropical Land Temperature-Precipitation Sensitivity Reveals Decreasing Evapotranspiration and Faster Vegetation Greening in {{CMIP6}} Projections},
  author = {Zhu, Boyuan and Cheng, Yongzhou and Hu, Xuyue and Chai, Yuanfang and Berghuijs, Wouter R. and Borthwick, Alistair G. L. and Slater, Louise},
