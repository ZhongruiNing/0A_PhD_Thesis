@article{zhouGlobalPatternEffect2015,
  title = {Global Pattern for the Effect of Climate and Land Cover on Water Yield},
  author = {Zhou, Guoyi and Wei, Xiaohua and Chen, Xiuzhi and Zhou, Ping and Liu, Xiaodong and Xiao, Yin and Sun, Ge and Scott, David F. and Zhou, Shuyidan and Han, Liusheng and Su, Yongxian},
  year = {2015},
