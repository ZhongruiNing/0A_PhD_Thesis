@article{dasTrendsTemperaturePrecipitation2022,
  title = {Trends in {{Temperature}}, {{Precipitation}}, {{Potential Evapotranspiration}}, and {{Water Availability}} across the {{Teesta River Basin}} under 1.5 and 2 Degrees {{C Temperature Rise Scenarios}} of {{CMIP6}}},
  author = {Das, Soumik and Datta, Pritha and Sharma, Dreamlee and Goswami, Kishor},
  year = {2022},
  month = jun,
  journal = {Atmosphere},
  volume = {13},
