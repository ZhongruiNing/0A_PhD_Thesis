@article{berghuijsPrecipitationShiftSnow2014,
  title = {A Precipitation Shift from Snow towards Rain Leads to a Decrease in Streamflow},
  author = {Berghuijs, W. R. and Woods, R. A. and Hrachowitz, M.},
  year = {2014},
