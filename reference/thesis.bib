  lccn = {1},
  keywords = {/unread,agriculture,Change points (CPs),China,extreme precipitation,Heavy precipitation,increase,pearl river-basin,rainfall,regression,sensitivity,spatiotemporal variations,Summer   temperature,temporal variability,Trend analyses,trends},
