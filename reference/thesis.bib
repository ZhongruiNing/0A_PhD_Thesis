@article{nazeerChangesHydroclimaticRegime2022,
  title = {Changes in the Hydro-Climatic Regime of the {{Hunza Basin}} in the {{Upper Indus}} under {{CMIP6}} Climate Change Projections},
  author = {Nazeer, Aftab and Maskey, Shreedhar and Skaugen, Thomas and McClain, Michael E.},
  year = {2022},
  month = dec,
  journal = {Scientific Reports},
  volume = {12},
