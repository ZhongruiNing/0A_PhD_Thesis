@article{rantanenArcticHasWarmed2022,
  title = {The {{Arctic}} Has Warmed Nearly Four Times Faster than the Globe since 1979},
  author = {Rantanen, Mika and Karpechko, Alexey Yu and Lipponen, Antti and Nordling, Kalle and Hyvarinen, Otto and Ruosteenoja, Kimmo and Vihma, Timo and Laaksonen, Ari},
  year = {2022},
