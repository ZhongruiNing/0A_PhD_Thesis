}

@article{sunImpactEnvironmentalChange2019,
  title = {Impact of Environmental Change on Runoff in a Transitional Basin: {{Tao River Basin}} from the {{Tibetan Plateau}} to the {{Loess Plateau}}, {{China}}},
  shorttitle = {Impact of Environmental Change on Runoff in a Transitional Basin},
  author = {Sun, Long and Wang, Yue-Yang and Zhang, Jian-Yun and Yang, Qin-Li and Bao, Zhen-Xin and Guan, Xiao-Xiang and Guan, Tie-Sheng and Chen, Xin and Wang, Guo-Qing},
