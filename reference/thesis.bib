@article{hanOneParameterAnalyticalDerivation2023,
  title = {One-{{Parameter Analytical Derivation}} in {{Modified Budyko Framework}} for {{Unsteady-State Streamflow Elasticity}} in {{Humid Catchments}}},
  author = {Han, Peng-Fei and Sankarasubramanian, Arumugam and Wang, Xu-Sheng and Wan, Li and Yao, Lili},
  year = {2023},
