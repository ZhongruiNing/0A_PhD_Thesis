  lccn = {2},
  keywords = {/unread,BIAS CORRECTION,Chinese basins,CLIMATE-CHANGE IMPACT,CMIP6,DROUGHT,EDCDFm,Future hydrological changes,HYDROLOGICALLY BASED DATASET,LAND-SURFACE FLUXES,MODEL,RIVER-BASIN,SOIL-MOISTURE,STATES,VIC model,WATER},
