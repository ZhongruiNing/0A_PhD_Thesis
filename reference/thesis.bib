@article{schillerbergGlobalAssessmentCompound2024,
  title = {Global {{Assessment}} of {{Compound Climate Extremes}} and {{Exposures}} of {{Population}}, {{Agriculture}}, and {{Forest Lands Under Two Climate Scenarios}}},
  author = {Schillerberg, Tayler A. and Tian, Di},
  year = {2024},
  journal = {Earth's Future},
