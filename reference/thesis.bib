@article{chenImpactClimateChange2023,
  title = {Impact of {{Climate Change}} on the {{Hydrological Regimes}} of the {{Midstream Section}} of the {{Yarlung Tsangpo River Basin Based}} on {{SWAT Model}}},
  author = {Chen, Yao and Wang, Li and Shi, Xiaonan and Zeng, Chen and Wang, Yuchun and Wang, Guanxing and Qiangba, Cicheng and Yue, Caiyun and Sun, Zugang and Renzeng, Ouzhu and Zhang, Fan},
  year = {2023},
  month = feb,
  journal = {Water},
  volume = {15},
  number = {4},
  pages = {685},
  publisher = {MDPI},
  address = {Basel},
  issn = {2073-4441},
  doi = {10.3390/w15040685},
  urldate = {2023-10-01},
  abstract = {Water resources and the water cycle in high mountain areas are significantly impacted by climate change. In this study, the midstream section of the Yarlung Tsangpo River basin, situated in the southern part of the Tibetan Plateau, was chosen as the target area, and the Soil Water Assessment Tool (SWAT) was used to assess how climate change may affect hydrological processes. The SWAT model proved effective for runoff and snow cover area simulation. Surface runoff, interflow, and groundwater accounted for 47.2\%, 24.4\%, and 28.4\% of the total runoff, respectively. The spatial distribution of runoff was mainly influenced by precipitation and glacier distribution, whereas the spatial distributions of individual runoff components were mainly influenced by soil properties. Overall, the total runoff as well as its components (surface runoff, interflow, and groundwater) increased at a rate of 0.03-0.83\%/10 yr (p {$>$} 0.05) in the study area during 1983-2017, which could be attributed to the increase in precipitation. Surface runoff peaked earlier (August) than interflow and groundwater (September), owing to the longer convergence time of interflow and groundwater. Future predictions showed a warming and wetting trend (p {$<$} 0.05) in the study area from 2020 to 2100 under the SSP1-2.6, SSP2-4.5, SSP3-7.0, and SSP5-8.5 scenarios. The total runoff was projected to increase at a rate of 0.92-3.56\%/10 yr, and the change of total runoff mainly came from the increase of surface runoff.},
  langid = {english},
  lccn = {3},
  keywords = {/unread,climate change,CMIP6,FLOW,GROUNDWATER,hydrological process,LAND-USE,MELT,RUNOFF,SIMULATION,SWAT,TIBETAN PLATEAU,UPSTREAM,WATER,Yarlung Tsangpo River},
  annotation = {Web of Science ID: WOS:000942380000001},
  file = {D:\zotero\storage\6CKY2USV\Chen 等 - 2023 - Impact of Climate Change on the Hydrological Regim.pdf}
}

@article{chenImpactsLandUse2020,
  title = {Impacts of Land Use Change and Climatic Effects on Streamflow in the {{Chinese Loess Plateau}}: {{A}} Meta-Analysis},
  shorttitle = {Impacts of Land Use Change and Climatic Effects on Streamflow in the {{Chinese Loess Plateau}}},
  author = {Chen, Hao and Fleskens, Luuk and Baartman, Jantiene and Wang, Fei and Moolenaar, Simon and Ritsema, Coen},
  year = {2020},
  month = feb,
  journal = {Science of the Total Environment},
  volume = {703},
  pages = {134989},
  publisher = {Elsevier},
  address = {Amsterdam},
  issn = {0048-9697, 1879-1026},
  doi = {10.1016/j.scitotenv.2019.134989},
  urldate = {2024-01-05},
  abstract = {Land use and climate change are recognized as two major drivers affecting surface streamflow. On the Chinese Loess Plateau, implementation of several land restoration projects has changed land cover in recent decades. The main objectives of this study were to understand how streamflow evolved on the Loess Plateau and how land use and climate change have contributed to this change. In this study, we selected 22 hydrological modelling studies covering 25 different watersheds in the Loess Plateau and we performed a meta-analysis by using the hydrological and meteorological data collected from these studies. The results indicate a streamflow decrease in 41 of a total of 52 case studies whereas precipitation change was found to be non-significant in the majority of the cases. Streamflow reduction was estimated to be -0.46 mm/year by meta-analysis across all case studies. Land use change was estimated to have 63.52\% impact on the streamflow reduction whereas climate change accounted for 36.48\% of the impact. Using meta-regression, an increasing soil and water conservation area was found to be positively correlated to streamflow reduction. We conclude that in the Chinese Loess Plateau, streamflow shows a decreasing trend and land restoration is the major cause of this reduction. To the knowledge of the authors, this is the first study that estimates streamflow dynamics across many watersheds on the entire Loess Plateau. (C) 2019 The Author(s). Published by Elsevier B.V.},
  langid = {english},
  keywords = {CATCHMENT,Climate change,ECOLOGICAL RESTORATION,Land restoration,MANN-KENDALL,Meta-regression,RESOURCES,RUNOFF,SOIL-WATER STORAGE,SPEARMANS RHO TESTS,Streamflow reduction,VARIABILITY,VEGETATION RESTORATION,WEI RIVER-BASIN},
  annotation = {Web of Science ID: WOS:000505924300090},
  file = {D:\zotero\storage\4H4YBSR8\Chen et al_2020_Impacts of land use change and climatic effects on streamflow in the Chinese.pdf}
}

